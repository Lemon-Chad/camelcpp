!ComplexNumber {
    !real(param)
    !imaginary(param)

    !str {
        return("\{real} + \{imaginary}i")
    }

    !add {
        !other(param)
        !r(other.real + real)
        !i(other.imaginary + imaginary)
        return(ComplexNumber(r, i))
    }

    return(self)
}


!x(ComplexNumber(3, 5))
!y(ComplexNumber(5, 2))
!z(x.add(y))

printLine("(\{x.str()}) + (\{y.str()}) = \{z.str()}")